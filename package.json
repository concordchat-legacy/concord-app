{
  "name": "silicon",
  "version": "1.0.0",
  "description": "Concord Website",
  "author": "Concord, Inc.",
  "license": "MIT",
  "scripts": {
    "dev:home": "parcel pages/home/* --lazy",
    "dev:app": "parcel pages/app/* --lazy",
    "build": "yarn build:home && yarn build:app && yarn build:assets",
    "build:home": "parcel build pages/home/* --no-source-maps",
    "build:app": "parcel build pages/app/* --dist-dir dist/app --no-source-maps",
    "build:assets": "parcel build pages/assets/* --dist-dir dist/assets --no-source-maps"
  },
  "private": true,
  "devDependencies": {
    "@parcel/transformer-sass": "^2.5.0",
    "@tauri-apps/cli": "^1.0.0-rc.10",
    "parcel": "^2.5.0"
  },
  "dependencies": {
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.4.0",
    "preact": "^10.7.2"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  }
}
